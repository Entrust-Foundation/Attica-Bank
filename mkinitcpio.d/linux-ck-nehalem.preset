# mkinitcpio preset file for the 'linux-ck-nehalem' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-ck-nehalem"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-ck-nehalem.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-ck-nehalem-fallback.img"
fallback_options="-S autodetect"

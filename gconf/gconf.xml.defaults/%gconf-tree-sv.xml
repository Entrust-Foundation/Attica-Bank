<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="firestarter">
				<dir name="firewall">
					<entry name="int_if">
						<local_schema short_desc="Det interna gränssnittet">
							<longdesc>Det nätverksgränssnitt som är ansluten till det lokala nätverket. Till exempel eth0 eller eth1.</longdesc>
						</local_schema>
					</entry>
					<entry name="nat">
						<local_schema short_desc="Aktivera NAT">
							<longdesc>Aktivera översättning av nätverksadresser (NAT) eller delning av Internetanslutning.</longdesc>
						</local_schema>
					</entry>
					<entry name="ext_if">
						<local_schema short_desc="Det externa gränssnittet">
							<longdesc>Det nätverksgränssnitt som är anslutet till Internet. Till exempel ppp0 eller eth0.</longdesc>
						</local_schema>
					</entry>
					<dir name="icmp">
						<entry name="enable">
							<local_schema short_desc="Aktivera konfigurationen av ICMP">
								<longdesc>Verkställ ICMP-inställningarna som är konfigurerade i guiden.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="tos">
						<entry name="enable">
							<local_schema short_desc="Aktivera konfigurationen av tos">
								<longdesc>Verkställ inställningarna i Typ av tjänst så som de är konfigurerade i guiden.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
				<dir name="client">
					<entry name="system_log">
						<local_schema short_desc="Systemloggfilen">
							<longdesc>Platsen för den fil som systemloggningsdemonen skriver till.</longdesc>
						</local_schema>
					</entry>
					<entry name="start_firewall_on_gui">
						<local_schema short_desc="Starta brandväggen när programmet startar">
							<longdesc>Starta brandväggen när programmet startar</longdesc>
						</local_schema>
					</entry>
					<entry name="first_run">
						<local_schema>
							<longdesc>Första gången Firestarter startas vidtas flera speciella åtgärder.</longdesc>
						</local_schema>
					</entry>
					<dir name="ui">
						<entry name="hitview_service_col">
							<local_schema short_desc="Visa tjänstekolumnen i träffvyn">
								<longdesc>Visa tjänstekolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_protocol_col">
							<local_schema short_desc="Visa protokollkolumnen i träffvyn">
								<longdesc>Visa protokollkolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_tos_col">
							<local_schema short_desc="Visa tos-kolumnen i träffvyn">
								<longdesc>Visa Typ av tjänst-kolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_length_col">
							<local_schema short_desc="Visa längdkolumnen i träffvyn.">
								<longdesc>Visa paketlängdskolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_destination_col">
							<local_schema short_desc="Visa destinationskolumnen i träffvyn">
								<longdesc>Visa destinationskolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_source_col">
							<local_schema short_desc="Visa källkolumnen i träffvyn">
								<longdesc>Visa källkolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_port_col">
							<local_schema short_desc="Visa portkolumnen i träffvyn">
								<longdesc>Visa portkolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_out_col">
							<local_schema short_desc="Visa utkolumnen i träffvyn">
								<longdesc>Visa den enhet som träffen dirigerades till i träffvyn.</longdesc>
							</local_schema>
						</entry>
						<entry name="hitview_in_col">
							<local_schema short_desc="Visa inkolumnen i träffvyn.">
							</local_schema>
						</entry>
						<entry name="hitview_time_col">
							<local_schema short_desc="Visa tidskolumnen i träffvyn">
								<longdesc>Visa tidskolumnen i träffvyn.</longdesc>
							</local_schema>
						</entry>
					</dir>
					<dir name="filter">
						<entry name="redundant">
							<local_schema short_desc="Filtrera bort identiska träffar">
								<longdesc>Filtrera bort sekvenser med identiska träffar.</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
